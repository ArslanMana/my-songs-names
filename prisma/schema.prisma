generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
 
}

model User {
  id        String     @id @default(cuid())
  name      String
  email     String     @unique
  password  String
  favorites Favorite[]
}

model Favorite {
  id        String   @id @default(cuid())
  songName  String
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}
